{
   type         = "delimited-text",
   format       = "CSV",
   id-field     = "toString($i)",
   fields = [
     { name = "i",       transform = "$1::double" },
     { name = "numbers", transform = "parseList('int', $2, 'y')"},
     { name = "time",    transform = "$3::date" },
     { name = "lon",     transform = "$4::double" },
     { name = "lat",     transform = "$5::double" },
     { name = "geom",    transform = "point($lon, $lat)" }
   ]
 }
